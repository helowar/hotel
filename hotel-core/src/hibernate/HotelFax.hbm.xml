<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.mangocity.hotel.order.persistence">
   <class name="HotelFax" table="or_hotel_fax" lazy="false"  dynamic-insert="true" dynamic-update="true">
	<id name="ID" column="auditHotelId">
			<generator class="sequence">
				<param name="sequence">seq_order_faxhotel</param>
			</generator>
	</id>      
    
         <property name="hotelId" />
         <property name="hotelName" />
         <property name="hotelFax" />
         <property name="faxNo" />
    
   </class>

   
</hibernate-mapping>
